{
  "compilerOptions": {
    "module": "ES2022",
    "target": "esnext",
    "lib": ["esnext", "DOM"],
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react-jsx"
  }
}
